{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"allowJs": true,
		"checkJs": false,
		"jsx": "react",
		"outDir": "./dist",
		"rootDir": "./",
		"removeComments": true,
		"noEmit": false,
		"pretty": true,
		"skipLibCheck": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true
	},
	"ts-node": {
		"compilerOptions": {
			"module": "CommonJS"
		}
	}
}
